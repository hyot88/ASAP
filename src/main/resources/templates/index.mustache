</div>
{{>layout/footerIndex}}

<script type="text/template" id="rankBox-template">
  <div class="default">
    <div class="overlap-group-container">
      <div class="overlap-group1-1">
        <div class="text-2">티어<br/>아이콘</div>
      </div>
      <div class="overlap-group2-1">
        <div class="txt_nickname">
          <div class="nickname">{nickname}</div>
        </div>
        <div class="layout_zone">
          <img src="/images/index/tier/{tier}.png" width="100%"/>
        </div>
      </div>
    </div>
    <div class="layout_exp">
      <div class="txt_-container">
        <div class="txt_exp notosanskr-normal-white-12px">EXP</div>
        <div class="txt_next_zone">
          <div class="txt_next_zone-item notosanskr-normal-white-12px">다음 ZONE</div>
          <div class="master-3">&nbsp;{nextTier}</div>
          <div class="txt_next_zone-item notosanskr-normal-white-12px">까지</div>
        </div>
      </div>
      <div class="overlap-group-1">
        <div class="gage_in">
          <div class="box_filled" style="width: {tierPoint}%"></div>
        </div>
        <div class="txt_exp_remain notosanskr-medium-white-10px">{tierPoint} pt</div>
        <div class="txt_exp_gained notosanskr-medium-white-10px">100 pt</div>
      </div>
    </div>
  </div>
</script>

<script type="text/template" id="noProgressMissionBox-template">

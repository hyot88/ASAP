    <div class="recently-added valign-text-bottom notosanskr-black-white-28px">Tick ㅡ</div>
    <div class="flex-row">
      <div class="flex-row-item mission_day day1" data-day="1">
        <img src="/images/index/mission/mission_day_1.png" width="100%"/>
      </div>
      <div class="flex-row-item mission_day day3" data-day="3">
        <img src="/images/index/mission/mission_day_3.png" width="100%"/>
      </div>
    </div>
  </div>
  <div class="mission_row">
    <div class="recently-added valign-text-bottom notosanskr-black-white-28px">Quick ㅡ</div>
    <div class="flex-row">
      <div class="flex-row-item mission_day day5" data-day="5">
        <img src="/images/index/mission/mission_day_5.png" width="100%"/>
      </div>
      <div class="flex-row-item mission_day day7" data-day="7">
        <img src="/images/index/mission/mission_day_7.png" width="100%"/>
      </div>
    </div>
  </div>
  <div class="mission_row">
    <div class="recently-added-1 valign-text-bottom">ㅡ Huick ㅡ</div>
    <div class="flex-row">
      <div class="flex-row-item mission_day half">
        <img src="/images/index/mission/mission_day_half.png" width="100%"/>
      </div>
      <div class="flex-row-item mission_day full">
        <img src="/images/index/mission/mission_day_full.png" width="100%"/>
      </div>
    </div>
  </div>
</script>

<script type="text/template" id="progressMissionBox-template">
  <div class="mission_row">
    <div class="recently-added-1 valign-text-bottom">ㅡ Huick ㅡ</div>
    <div class="flex-row">
      <div class="flex-row-item mission_day half">
        <img src="/images/index/mission/mission_day_half.png" width="100%"/>
      </div>
      <div class="flex-row-item mission_day full">
        <img src="/images/index/mission/mission_day_full.png" width="100%"/>
      </div>
    </div>
